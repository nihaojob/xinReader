{
	"manifest_version": 2,
	"name": "昕阅",
	"version": "1.0.1",
	"description": "kindle笔记展示工具，展示你想记的内容。",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "昕阅，展示你想记的内容",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/_/chrome/newtab*"
			],
			"js": [
				"js/jquery-1.8.3.js",
				"js/newTab.js",
				"js/kindle.js"
			],
			"css": [
				"css/tailwind.min.css"
			],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"storage"
	],
	"homepage_url": "https://github.com/nihaojob/xinReader",
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"omnibox": {
		"keyword": "go"
	}
}